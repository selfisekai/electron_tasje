{
    "name": "electron_tasje",
    "description": "Packs Electron apps",
    "version": "2.1.3.7-jp2",
    "build": {
        "files": [
            "build/bundle.aoeuid.js",
            {
                "from": "build",
                "to": "cuild"
            }
        ],
        "productName": "Tasje",
        "linux": {
            "executableName": "tasje",
            "category": "Tools",
            "icon": "icons_linux",
            "desktop": {
                "CustomField": "custom_value"
            },
            "protocols": [
                {
                    "name": "tasje",
                    "schemes": [
                        "tasje"
                    ]
                },
                {
                    "name": "ebuilder",
                    "schemes": [
                        "ebuilder",
                        "electron-builder"
                    ]
                }
            ]
        },
        "protocols": {
            "name": "ignored",
            "schemes": [
                "invalid"
            ]
        },
        "fileAssociations": {
            "ext": "tas",
            "mimeType": "application/x-tas"
        }
    }
}